
{
    "manifest_version": 2,
    "name": "AddQuery",
    "description": "Original Chrome Extension",
    "version": "1.0",
    "browser_action": {
      "default_icon": "icon.png"
    },
    "content_scripts": [
        {
          "matches": ["https://www.google.com/*", "https://www.youtube.com/*"],
          "js": ["script.js"]
        }
    ]
  }